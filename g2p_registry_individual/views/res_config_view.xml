<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="gender_inherit_res_config_settings_view_form" model="ir.ui.view">
        <field name="name">gender.res.config.settings.view.inherit.setup</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="999" />
        <field name="inherit_id" ref="g2p_registry_base.openg2p_inherit_res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@name='validation_container']" position="after">
                    <div class="row mt16 o_settings_container" name="gender_container">
                        <div class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_left_pane">
                            </div>
                            <div class="o_setting_right_pane">
                                <div>
                                    <field name="gender_config" />
                                    <div>
                                        <label for="gender_config" string="Gender Configuration" />
                                    </div>
                                    <span class="text-muted">Set up gender list.</span>
                                </div>
                            </div>
                        </div>
                    </div>
            </xpath>
        </field>
    </record>
</odoo>
